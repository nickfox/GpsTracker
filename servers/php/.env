# Application Settings
APP_NAME="GPS Tracker"
APP_ENV=production
APP_DEBUG=true
APP_TIMEZONE=UTC

# MySQL Database Settings
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=YOUR_DATABASE_NAME
DB_USERNAME=YOUR_DATABASE_USERNAME
DB_PASSWORD=YOUR_PASSWORD
DB_TABLE_PREFIX=YOUR_TABLE_PREFIX

# Maps Configuration
MAP_PROVIDER=openstreetmap
GOOGLE_MAPS_KEY=YOUR_GOOGLE_MAP_KEY

# Logging
LOG_CHANNEL=stderr
LOG_LEVEL=error

# Units
DISTANCE_UNIT=mi
SPEED_UNIT=mph
